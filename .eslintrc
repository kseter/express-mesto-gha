{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
      "no-underscore-dangle": ["error",{"allow":["_id"]}],
      "semi": 0,
      "keyword-spacing": 0,
      "indent": 0,
      "arrow-body-style": 0,
      "consistent-return": 0
    }
}
